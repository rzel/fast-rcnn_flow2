train_net: "train.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 30000
display: 5
average_loss: 100
# iter_size: 2
momentum: 0.9
weight_decay: 0.0005
# We disable standard caffe solver snapshotting and implement our own snapshot
# function
snapshot: 0
# We still use the snapshot prefix, though
snapshot_prefix: "/nfs.yoda/xiaolonw/imagenet/data/models/std_try_fcrand3/vgg16_fast_rcnn"
